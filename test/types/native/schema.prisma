datasource db {
  provider = "postgresql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/prisma/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "db"
  previewFeatures = ["nativeTypes"]
}

model User {
  id String @id @default(cuid())
  a  String @db.VarChar(5)
  b  String? @db.VarChar(50)
}
