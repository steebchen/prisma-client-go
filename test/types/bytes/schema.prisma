datasource db {
  provider = "postgresql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/prisma/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "db"
}

model User {
  id       String @id @default(cuid())
  bytes    Bytes
  bytesOpt Bytes?
}
