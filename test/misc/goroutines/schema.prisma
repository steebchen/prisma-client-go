datasource db {
  provider = "postgresql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/steebchen/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "db"
}

model User {
  id String  @id @default(cuid()) @map("_id")
  a  String  @db.VarChar(5)
  b  String? @db.VarChar(50)
}
