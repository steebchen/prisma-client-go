datasource db {
  provider = "sqlite"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/steebchen/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "db"
  previewFeatures   = ["prismaSchemaFolder"]
}

model noop {
  id String @id @default(cuid()) @map("_id")
}
