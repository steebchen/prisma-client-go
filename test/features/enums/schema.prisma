datasource db {
  provider = "postgresql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/steebchen/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "enums"
}

enum ServerOS {
  Unknown
  Linux
  Windows
  Mac
}

model User {
  id       String    @id @default(cuid()) @map("_id")
  role     Role
  roleOpt  Role?
  stuff1   Stuff?
  stuff2   Stuff?
  stuff3   Stuff?
  serverOS ServerOS?
}

enum Role {
  User
  Moderator
  Admin
}

enum Stuff {
  different
  CASING
  HaHa
  last7d
  last30D
  last7dAnd
  last30DAnd
  Slack
  id
}
