datasource db {
	provider = "sqlite"
	url      = env("__REPLACE__")
}

generator db {
	provider = "go run github.com/prisma/prisma-client-go"
	output = "./db_gen.go"
	package = "composite"
}

model User {
	firstName  String
	middleName String
	lastName   String
	@@id([firstName, lastName], name: "AllID")
	@@unique([firstName, middleName, lastName], name: "AllUnique")
}
