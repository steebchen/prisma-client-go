datasource db {
  provider = "sqlite"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/prisma/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "composite"
}

model User {
  firstName  String
  middleName String
  lastName   String

  @@id([firstName, lastName])
  @@unique([firstName, middleName, lastName])
}
